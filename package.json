{
  "name": "@kikobeats/map-keys-deep",
  "description": "Recursive lodash.mapKeys",
  "homepage": "https://github.com/Kikobeats/map-keys-deep",
  "version": "1.0.5",
  "main": "index.js",
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://kikobeats.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kikobeats/map-keys-deep.git"
  },
  "bugs": {
    "url": "https://github.com/Kikobeats/map-keys-deep/issues"
  },
  "keywords": [
    "deep",
    "keys",
    "lodash",
    "map",
    "recursive"
  ],
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "c8": "latest",
    "ci-publish": "latest",
    "github-generate-release": "latest",
    "coveralls": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "git-dirty": "latest",
    "simple-git-hooks": "latest",
    "nano-staged": "latest",
    "lodash": "latest",

    "nyc": "latest",
    "@ksmithut/prettier-standard": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "standard-version": "latest"
  },
  "engines": {
    "node": ">= 8"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "standard-markdown README.md && standard",
    "postrelease": "npm run release:tags && npm run release:github && ci-publish --access=public",

    "pretest": "npm run lint",
    "release": "git-authors-cli && finepack && git add package.json && standard-version -a",
    "release:github": "github-generate-release",
    "release:tags": "git push --follow-tags origin HEAD:master",
    "test": "c8 ava",
    "update": "ncu -a"
  },
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0
      ]
    }
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit",
    "pre-commit": "npx nano-staged"
  },
  "nano-staged": {
    "linters": {
      "package.json": [
        "finepack",
        "git add"
      ],
      "*.js": [
        "prettier-standard",
        "git add"
      ],
      "*.md": [
        "standard-markdown",
        "git add"
      ]
    }
  }
}
